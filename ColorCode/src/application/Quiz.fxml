            <DropShadow blurType="ONE_PASS_BOX" color="#00000080" />
         </effect>
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="submitBtn" layoutX="359.0" layoutY="634.0" mnemonicParsing="false" onAction="#submit" prefHeight="61.0" prefWidth="190.0" text="SUBMIT">
         <font>
            <Font name="Lucida Bright Demibold Italic" size="35.0" />
         </font>
      </Button>
      <Label layoutX="64.0" layoutY="347.0" prefHeight="47.0" prefWidth="780.0" text="Choose colors to mix and click submit.">
         <font>
            <Font name="Lucida Bright Demibold Italic" size="40.0" />
         </font>
      </Label>
      <Label layoutX="292.0" layoutY="65.0" text="What colors make ">
         <font>
            <Font name="Lucida Bright Demibold Italic" size="34.0" />
         </font>
      </Label>
      <Rectangle fx:id="randomColor" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="100.0" layoutX="385.0" layoutY="196.0" stroke="BLACK" strokeType="INSIDE" width="135.0" />
      <ImageView fitHeight="88.0" fitWidth="65.0" layoutX="488.0" layoutY="668.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="https://raw.githubusercontent.com/ksi616/ColorCodeProject/master/Submit.png" />
         </image>
      </ImageView>
      <Label layoutX="710.0" layoutY="684.0" text="Go Back!">
         <font>
            <Font name="Lucida Bright Demibold Italic" size="30.0" />
         </font>
      </Label>
	</children>
