            <DropShadow blurType="ONE_PASS_BOX" color="#00000080" />
         </effect>
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="blueBtn" alignment="CENTER" layoutX="129.0" layoutY="446.0" mnemonicParsing="false" onAction="#blueBtnToggle" prefHeight="100.0" prefWidth="135.0" stylesheets="application/application.css" text="BLUE" textFill="#dddddd">
         <effect>
            <DropShadow blurType="ONE_PASS_BOX" color="#00000080" />
         </effect>
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="yellowBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="619.0" layoutY="446.0" mnemonicParsing="false" onAction="#yellowBtnToggle" prefHeight="100.0" prefWidth="133.0" stylesheets="application/application.css" text="YELLOW" textFill="#dddddd">
