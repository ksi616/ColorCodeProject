<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="800.0" minWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.QuizController">
	<children>
      <ImageView fitHeight="800.0" fitWidth="901.0" opacity="0.89" pickOnBounds="true">
         <image>
            <Image url="https://raw.githubusercontent.com/ksi616/ColorCodeProject/master/background.jpg" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f873eb" height="54.0" layoutX="285.0" layoutY="59.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="4.0" width="332.0" />
      <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="726.0" layoutY="590.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="https://raw.githubusercontent.com/ksi616/ColorCodeProject/master/Back%20button.png" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#78e0ff" height="54.0" layoutX="54.0" layoutY="344.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="4.0" width="796.0" />
				<Button fx:id="homeBtn" layoutX="726.0" layoutY="590.0" mnemonicParsing="false" onAction="#onHome" opacity="0.0" prefHeight="100.0" prefWidth="100.0" text="HOME">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="redBtn" alignment="CENTER" layoutX="386.0" layoutY="446.0" mnemonicParsing="false" onAction="#redBtnToggle" prefHeight="100.0" prefWidth="135.0" stylesheets="application/application.css" text="RED" textFill="#dddddd">
